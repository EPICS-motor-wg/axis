          <value>
            <color name="Minor" red="255" green="128" blue="0" />
          </value>
        </exp>
        <exp bool_exp="pv2 != 0 || pv3 != 0">
          <value>
            <color name="Major" red="255" green="0" blue="0" />
          </value>
        </exp>
        <exp bool_exp="true">
          <value>
            <color red="0" green="255" blue="0" />
          </value>
        </exp>
        <pv trig="true">$(MPV).DMOV</pv>
        <pv trig="true">$(MPV).MOVN</pv>
        <pv trig="true">$(MPV).STAT</pv>
        <pv trig="true">$(MPV).MISS</pv>
        <pv trig="true">$(MPV).LVIO</pv>
      </rule>
      <rule name="message-text" prop_id="text" out_exp="false">
        <exp bool_exp="pv5 != 0 || pv6 != 0">
          <value>HOMING</value>
        </exp>
        <exp bool_exp="pv0 == 0 || pv1 != 0">
          <value>MOVING</value>
        </exp>
        <exp bool_exp="pv3 != 0">
          <value>MISSED</value>
        </exp>
        <exp bool_exp="( pv4 &amp; 16384 ) == 0">
          <value>NOT HOMED</value>
        </exp>
        <exp bool_exp="( pv4 &amp; 64 ) != 0">
          <value>SLIP/STALL</value>
        </exp>
        <exp bool_exp="( pv4 &amp; 512 ) != 0">
          <value>PROBLEM</value>
        </exp>
        <exp bool_exp="pv7 != 0">
          <value>LIMIT VIOLATION</value>
        </exp>
        <exp bool_exp="pv2 != 0">
          <value>pvStr2</value>
        </exp>
        <exp bool_exp="true">
          <value>IDLE</value>
        </exp>
        <pv trig="true">$(MPV).DMOV</pv>
        <pv trig="true">$(MPV).MOVN</pv>
        <pv trig="true">$(MPV).STAT</pv>
        <pv trig="true">$(MPV).MISS</pv>
        <pv trig="true">$(MPV).MSTA</pv>
        <pv trig="true">$(MPV).HOMF</pv>
        <pv trig="true">$(MPV).HOMR</pv>
        <pv trig="true">$(MPV).LVIO</pv>
      </rule>
    </rules>
